<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="home.smart.fly.zhihuindex">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".settings.UserInf"
        android:allowBackup="true"
        android:icon="@mipmap/zhihu_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".settings.loginActivity"></activity>
        <activity android:name=".settings.findActivity"></activity>
        <activity android:name=".settings.registerActivity"></activity>
        <activity android:name=".activity.SearchActivity"></activity>
        <activity android:name=".activity.AnswerActivity"></activity>
        <activity android:name=".activity.AnswerListActivity"></activity>
        <activity android:name=".activity.ProblemCommitActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>